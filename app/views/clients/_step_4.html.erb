
<% if flash[:step_4_errors] %>
  <div class="w3-panel w3-red">
    <h3>Please correct the following fields to continue with the application</h3>
    <% flash[:step_4_errors].each do |message| %>
      <p><i class="fa fa-exclamation-circle"></i>         <%= message %></</p>
    <% end %>
  </div>
<% elsif flash[:step_3_success] %>
  <div class="w3-panel w3-green">
    <h3>Good Job!</h3>
    <p><i class="fa fa-check-circle"></i>        <%= flash[:step_3_success]%></p>
  </div>
<% end %>

<div class="w3-container w3-card-4">
  <div class="w3-row w3-field-padding">
    <h2 class="section-title">Step 4 - Post Payment</h2>
    <p class="section-sub-title margin-b-md">Here we gather the rest of the information to begin to process your application.</p>
    <h3 class="field-explanation-title margin-lg">Alternate Contact (Optional)</h3>
    <p class="field-explanation-sub-title margin-b-md">In case we are not able to reach you is there someone else we can contact?</p>
    <label>Full Name</label>
    <%= form.text_field :alternate_contact_name, id: :client_alternate_contact_name, class: "w3-input w3-border" %>
    <label>Phone Number</label>
    <%= form.text_field :alternate_contact_phone_number, id: :client_alternate_contact_phone_number, class: "w3-input w3-border" %>
    <label>Email</label>
    <%= form.text_field :alternate_contact_email, id: :client_alternate_contact_email, class: "w3-input w3-border" %>
    <p class="section-sub-title-left margin-md margin-b-sm">Best Method to Contact This Person</p>
    <p>
      <%= form.radio_button :alternate_contact_best_method, "phone", id: :client_alternate_contact_best_method_phone %>
      <label for="client_alternate_contact_best_method_phone">Phone</label>
    </p>
    <p>
      <%= form.radio_button :alternate_contact_best_method, "email", id: :client_alternate_contact_best_method_email %>
      <label for="client_alternate_contact_best_method_email">Email</label>
    </p>
    <p>
      <%= form.radio_button :alternate_contact_best_method, "both", id: :client_alternate_contact_best_method_both %>
      <label for="client_alternate_contact_best_method_both">Both</label>
    </p>
  </div>
</div>

