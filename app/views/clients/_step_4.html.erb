    <% else %>
      <% owes_money_papers.each do |option, symbol| %>
        <p><%= form.label option, class: "form-label" %><%= form.check_box "owes_money_papers][#{symbol}", class: "w3-input w3-border" %></p>
      <% end %>
    <% end %>
